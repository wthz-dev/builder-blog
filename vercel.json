{
    "functions": {
      "server/src/index.ts": {
        "runtime": "nodejs18.x"
      }
    },
    "rewrites": [
      { "source": "/meta/(.*)", "destination": "/server/src/index.ts" },
      { "source": "/api/(.*)", "destination": "/server/src/index.ts" },
      { "source": "/(.*)", "destination": "/index.html" }
    ]
  }